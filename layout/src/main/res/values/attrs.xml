        <attr name="layoutLabelTextSize" format="dimension|reference" />
        <attr name="layoutLabelStyle" format="reference" />
        <attr name="layoutBorderColor" format="color|reference" />
        <attr name="layoutBorderCornerRadius" format="dimension|reference" />
        <attr name="layoutBorderStrokeWidth" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="ShimmerSimpleLayout">
        <attr name="shimmer_md_color" format="color|reference"/>
        <attr name="shimmer_md_animation_duration" format="integer|reference"/>
        <attr name="shimmer_md_auto_start" format="boolean|reference"/>
        <attr name="shimmer_md_angle" format="integer|reference"/>
        <attr name="shimmer_md_mask_width" format="float|reference"/>
        <attr name="shimmer_md_gradient_center_color_width" format="float|reference"/>
        <attr name="shimmer_md_reverse_animation" format="boolean|reference"/>
    </declare-styleable>

    <declare-styleable name="TableLayout">
        <attr name="fhtl_min_scale" format="float|reference" />
        <attr name="fhtl_max_scale" format="float|reference" />
    </declare-styleable>

    <declare-styleable name="ToggleButtonLayout">
        <attr name="menu" format="reference" />
        <attr name="multipleSelection" format="boolean|reference" />
        <attr name="allowDeselection" format="boolean|reference" />
        <attr name="dividerColor" format="color|reference" />
        <attr name="selectedColor" format="color|reference" />
        <attr name="customLayout" format="reference|reference" />
        <attr name="toggleMode" format="enum">
            <enum name="wrap" value="0" />
            <enum name="even" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TicketLayout">
        <attr name="ticketOrientation" />
        <attr name="ticketBackgroundColor" />
        <attr name="ticketScallopRadius" />
        <attr name="ticketScallopPositionPercent" />
        <attr name="ticketShowBorder" />
        <attr name="ticketBorderWidth" />
        <attr name="ticketBorderColor" />
        <attr name="ticketShowDivider" />
        <attr name="ticketDividerWidth" />
        <attr name="ticketDividerPadding" />
        <attr name="ticketDividerColor" />
        <attr name="ticketDividerType" />
        <attr name="ticketDividerDashGap" />
        <attr name="ticketDividerDashLength" />
        <attr name="ticketCornerType" />
        <attr name="ticketCornerRadius" />
        <attr name="ticketElevation" />
        <attr name="android:elevation" />
        <attr name="ticketDividerDesign" format="enum">
            <enum name="color" value="0" />
            <enum name="punch" value="1" />
        </attr>
        <attr name="view" format="reference" />
        <attr name="ticketDividerLocation">
            <flag name="none" value="0" />
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
            <flag name="left" value="4" />
            <flag name="right" value="8" />
        </attr>
        <attr name="ticketPunchLocation">
            <flag name="none" value="0" />
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
            <flag name="left" value="4" />
            <flag name="right" value="8" />
        </attr>
        <attr name="ticketPunchRadius" format="dimension|reference" />
    </declare-styleable>

</resources>
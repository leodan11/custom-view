<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="circularBackgroundColor" format="color" />
        <attr name="shadow" format="boolean" />
        <attr name="shadow_color" format="color" />
        <attr name="shadow_radius" format="float" />
        <attr name="shadow_gravity">
            <flag name="center" value="1" />
            <flag name="top" value="2" />
            <flag name="bottom" value="3" />
            <flag name="start" value="4" />
            <flag name="end" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ColorProgressBar">
        <attr name="toProgressColor" format="color" />
    </declare-styleable>

    <declare-styleable name="DividerView">
        <attr name="toDividerColor" format="color" />
        <attr name="dividerDashLength" format="dimension" />
        <attr name="dividerDashGap" format="dimension" />
        <attr name="dividerDashThickness" format="dimension" />
        <attr name="toDividerOrientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ExpandableCardView">
        <attr name="expandTitle" format="string" />
        <attr name="expandIcon" format="reference" />
        <attr name="expandIconTint" format="color" />
        <attr name="expandArrowIcon" format="reference" />
        <attr name="expandArrowIconTint" format="color" />
        <attr name="innerViewExpand" format="reference" />
        <attr name="expandOnClick" format="boolean" />
        <attr name="animationExpandDuration" format="integer" />
        <attr name="startExpanded" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FabWithLabelView">
        <attr name="android:src" />
        <attr name="srcCompat" />
        <attr name="fabLabel" format="string" />
        <attr name="fabBackgroundColor" format="color" />
        <attr name="fabLabelColor" format="color" />
        <attr name="fabLabelBackgroundColor" format="color" />
        <attr name="fabLabelClickable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RecyclerViewSwipe">
        <attr name="leftBgColorSwipe" format="color" />
        <attr name="leftImageSwipe" format="reference" />
        <attr name="leftTextSwipe" format="string" />
        <attr name="rightBgColorSwipe" format="color" />
        <attr name="rightImageSwipe" format="reference" />
        <attr name="rightTextSwipe" format="string" />
        <attr name="textColorSwipe" format="color" />
        <attr name="textSizeSwipe" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SpeedDialView">
        <attr name="android:enabled" />
        <attr name="sdExpansionMode" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
        <attr name="sdMainFabAnimationRotateAngle" format="float" />
        <attr name="sdMainFabClosedSrc" format="reference" />
        <attr name="sdMainFabClosedIconColor" format="color" />
        <attr name="sdMainFabClosedBackgroundColor" format="color" />
        <attr name="sdMainFabOpenedSrc" format="reference" />
        <attr name="sdMainFabOpenedIconColor" format="color" />
        <attr name="sdMainFabOpenedBackgroundColor" format="color" />
        <attr name="sdOverlayLayout" format="reference" />
        <attr name="sdUseReverseAnimationOnClose" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SpeedDialOverlayLayout">
        <attr name="clickable_overlay" format="boolean" />
        <attr name="android:background" />
    </declare-styleable>

    <declare-styleable name="SignatureView">
        <attr name="toEnable" format="boolean" />
        <attr name="toPenSize" format="dimension" />
        <attr name="toPenColor" format="color" />
        <attr name="toBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SnowfallView">
        <attr name="snowflakesNum" format="integer" />
        <attr name="snowflakeImage" format="reference" />
        <attr name="snowflakeAlphaMin" format="integer" />
        <attr name="snowflakeAlphaMax" format="integer" />
        <attr name="snowflakeAngleMax" format="integer" />
        <attr name="snowflakeSizeMin" format="dimension" />
        <attr name="snowflakeSizeMax" format="dimension" />
        <attr name="snowflakeSpeedMin" format="integer" />
        <attr name="snowflakeSpeedMax" format="integer" />
        <attr name="snowflakesFadingEnabled" format="boolean" />
        <attr name="snowflakesAlreadyFalling" format="boolean" />
        <attr name="snowflakesMultipleImages" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TicketView">
        <attr name="ticketOrientation" />
        <attr name="ticketBackgroundColor" />
        <attr name="ticketScallopRadius" />
        <attr name="ticketScallopPositionPercent" />
        <attr name="ticketShowBorder" />
        <attr name="ticketBorderWidth" />
        <attr name="ticketBorderColor" />
        <attr name="ticketShowDivider" />
        <attr name="ticketDividerWidth" />
        <attr name="ticketDividerPadding" />
        <attr name="ticketDividerColor" />
        <attr name="ticketDividerType" />
        <attr name="ticketDividerDashGap" />
        <attr name="ticketDividerDashLength" />
        <attr name="ticketCornerType" />
        <attr name="ticketCornerRadius" />
        <attr name="ticketElevation" />
        <attr name="android:elevation" />
        <attr name="ticketBackgroundBeforeDivider" format="reference" />
        <attr name="ticketBackgroundAfterDivider" format="reference" />
        <attr name="ticketShadowColor" format="color" />
    </declare-styleable>

    <declare-styleable name="WaveLoadingView">
        <attr name="wlv_borderWidth" format="dimension"/>
        <attr name="wlv_borderColor" format="color"/>
        <attr name="wlv_progressValue" format="integer"/>
        <attr name="wlv_shapeType" format="enum">
            <enum name="triangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="square" value="2" />
            <enum name="rectangle" value="3" />
        </attr>
        <attr name="wlv_triangle_direction" format="enum">
            <enum name="north" value="0" />
            <enum name="south" value="1" />
            <enum name="east" value="2" />
            <enum name="west" value="3" />
        </attr>
        <attr name="wlv_round_rectangle" format="boolean"/>
        <attr name="wlv_round_rectangle_x_and_y" format="integer"/>
        <attr name="wlv_waveColor" format="color"/>
        <attr name="wlv_wave_background_Color" format="color"/>
        <attr name="wlv_waveAmplitude" format="float"/>
        <attr name="wlv_titleTop" format="string"/>
        <attr name="wlv_titleCenter" format="string"/>
        <attr name="wlv_titleBottom" format="string"/>
        <attr name="wlv_titleTopSize" format="dimension"/>
        <attr name="wlv_titleCenterSize" format="dimension"/>
        <attr name="wlv_titleBottomSize" format="dimension"/>
        <attr name="wlv_titleTopColor" format="color"/>
        <attr name="wlv_titleCenterColor" format="color"/>
        <attr name="wlv_titleBottomColor" format="color"/>
        <attr name="wlv_titleTopStrokeColor" format="color"/>
        <attr name="wlv_titleTopStrokeWidth" format="dimension"/>
        <attr name="wlv_titleBottomStrokeColor" format="color"/>
        <attr name="wlv_titleBottomStrokeWidth" format="dimension"/>
        <attr name="wlv_titleCenterStrokeColor" format="color"/>
        <attr name="wlv_titleCenterStrokeWidth" format="dimension"/>
    </declare-styleable>

</resources>
<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean|reference" />
        <attr name="border_width" format="dimension|reference" />
        <attr name="border_color" format="color|reference" />
        <attr name="circularBackgroundColor" format="color|reference" />
        <attr name="shadow" format="boolean|reference" />
        <attr name="shadow_color" format="color|reference" />
        <attr name="shadow_radius" format="float|reference" />
        <attr name="shadow_gravity">
            <flag name="center" value="1" />
            <flag name="top" value="2" />
            <flag name="bottom" value="3" />
            <flag name="start" value="4" />
            <flag name="end" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DividerView">
        <attr name="toDividerColor" format="color|reference" />
        <attr name="dividerDashLength" format="dimension|reference" />
        <attr name="dividerDashGap" format="dimension|reference" />
        <attr name="dividerDashThickness" format="dimension|reference" />
        <attr name="toDividerOrientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ExpandableCardView">
        <attr name="expandTitle" format="string|reference" />
        <attr name="expandIcon" format="reference" />
        <attr name="expandIconTint" format="color|reference" />
        <attr name="expandArrowIcon" format="reference" />
        <attr name="expandArrowIconTint" format="color|reference" />
        <attr name="innerViewExpand" format="reference" />
        <attr name="expandOnClick" format="boolean|reference" />
        <attr name="animationExpandDuration" format="integer|reference" />
        <attr name="startExpanded" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="FabWithLabelView">
        <attr name="android:src" />
        <attr name="srcCompat" />
        <attr name="fabLabel" format="string|reference" />
        <attr name="fabBackgroundColor" format="color|reference" />
        <attr name="fabLabelColor" format="color|reference" />
        <attr name="fabLabelBackgroundColor" format="color|reference" />
        <attr name="fabLabelClickable" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="GradientButton">
        <attr name="gradient_ui_startColor" />
        <attr name="gradient_ui_centerColor" />
        <attr name="gradient_ui_endColor" />
        <attr name="gradient_ui_colors" />
        <attr name="gradient_ui_orientation" />
        <attr name="gradient_ui_animate" />
        <attr name="gradient_ui_animationEnterDuration" />
        <attr name="gradient_ui_animationExitDuration" />
        <attr name="gradient_ui_cornerRadius" />
    </declare-styleable>

    <declare-styleable name="GradientCardView">
        <attr name="gradient_ui_startColor" />
        <attr name="gradient_ui_centerColor" />
        <attr name="gradient_ui_endColor" />
        <attr name="gradient_ui_colors" />
        <attr name="gradient_ui_orientation" />
    </declare-styleable>

    <declare-styleable name="GradientView">
        <attr name="gv_start" format="color|reference" />
        <attr name="gv_alpha_start" format="float|reference" />
        <attr name="gv_end" format="color|reference" />
        <attr name="gv_alpha_end" format="float|reference" />
        <attr name="gv_direction">
            <flag name="left_to_right" value="1" />
            <flag name="right_to_left" value="2" />
            <flag name="top_to_bottom" value="3" />
            <flag name="bottom_to_top" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NumberPicker">
        <attr name="snp_buttonIconDown" format="integer|reference" />
        <attr name="snp_buttonIconUp" format="integer|reference" />
        <attr name="snp_buttonIconLeft" format="integer|reference" />
        <attr name="snp_buttonIconRight" format="integer|reference" />
        <attr name="snp_buttonBackgroundTintSelector" format="integer|reference" />
        <attr name="snp_buttonTouchScaleFactor" format="float|reference" />

        <attr name="snp_buttonPaddingLeft" format="dimension|reference" />
        <attr name="snp_buttonPaddingRight" format="dimension|reference" />
        <attr name="snp_buttonPaddingTop" format="dimension|reference" />
        <attr name="snp_buttonPaddingBottom" format="dimension|reference" />

        <attr name="snp_value" format="integer|reference" />
        <attr name="snp_maxValue" format="integer|reference" />
        <attr name="snp_minValue" format="integer|reference" />
        <attr name="snp_stepSize" format="integer|reference" />
        <attr name="snp_value_text_size" format="dimension|reference" />
        <attr name="snp_value_text_color" format="color|reference" />
        <attr name="snp_value_text_appearance" format="reference" />

        <attr name="snp_updateInterval" format="integer|reference" />
        <attr name="snp_scrollEnabled" format="boolean|reference" />

        <attr name="snp_orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>

        <attr name="snp_valueMarginStart" format="dimension|reference" />
        <attr name="snp_valueMarginEnd" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="pinViewTheme">
        <attr name="pinViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="PinView">
        <attr name="pinCount" format="integer|reference" />
        <attr name="isPassword" format="boolean|reference" />
        <attr name="pinPadding" format="dimension|reference" />
        <attr name="pinBackground" format="reference" />
        <attr name="pinMask" format="string|reference" />
        <attr name="android:textSize" format="dimension|reference" />
        <attr name="android:fontFamily" format="string|reference" />
        <attr name="android:textColor" format="reference|color" />
        <attr name="android:textStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RecyclerViewSwipe">
        <attr name="leftBgColorSwipe" format="color|reference" />
        <attr name="leftImageSwipe" format="reference" />
        <attr name="leftTextSwipe" format="string|reference" />
        <attr name="rightBgColorSwipe" format="color|reference" />
        <attr name="rightImageSwipe" format="reference" />
        <attr name="rightTextSwipe" format="string|reference" />
        <attr name="textColorSwipe" format="color|reference" />
        <attr name="textSizeSwipe" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="SpeedDialView">
        <attr name="android:enabled" />
        <attr name="sdExpansionMode" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
        <attr name="sdMainFabAnimationRotateAngle" format="float|reference" />
        <attr name="sdMainFabClosedSrc" format="reference" />
        <attr name="sdMainFabClosedIconColor" format="color|reference" />
        <attr name="sdMainFabClosedBackgroundColor" format="color|reference" />
        <attr name="sdMainFabOpenedSrc" format="reference" />
        <attr name="sdMainFabOpenedIconColor" format="color|reference" />
        <attr name="sdMainFabOpenedBackgroundColor" format="color|reference" />
        <attr name="sdMenu" format="reference" />
        <attr name="sdOverlayLayout" format="reference" />
        <attr name="sdUseReverseAnimationOnClose" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="SpeedDialOverlayLayout">
        <attr name="clickable_overlay" format="boolean|reference" />
        <attr name="android:background" />
    </declare-styleable>

    <declare-styleable name="SignatureView">
        <attr name="toEnable" format="boolean|reference" />
        <attr name="toPenSize" format="dimension|reference" />
        <attr name="toPenColor" format="color|reference" />
        <attr name="toBackgroundColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="SnowfallView">
        <attr name="snowflakesNum" format="integer|reference" />
        <attr name="snowflakeImage" format="reference" />
        <attr name="snowflakeAlphaMin" format="integer|reference" />
        <attr name="snowflakeAlphaMax" format="integer|reference" />
        <attr name="snowflakeAngleMax" format="integer|reference" />
        <attr name="snowflakeSizeMin" format="dimension|reference" />
        <attr name="snowflakeSizeMax" format="dimension|reference" />
        <attr name="snowflakeSpeedMin" format="integer|reference" />
        <attr name="snowflakeSpeedMax" format="integer|reference" />
        <attr name="snowflakesFadingEnabled" format="boolean|reference" />
        <attr name="snowflakesAlreadyFalling" format="boolean|reference" />
        <attr name="snowflakesMultipleImages" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="SwitchIconView">
        <attr name="si_tint_color" format="color|reference" />
        <attr name="si_animation_duration" format="integer|reference" />
        <attr name="si_disabled_alpha" format="float|reference" />
        <attr name="si_disabled_color" format="color|reference" />
        <attr name="si_enabled" format="boolean|reference" />
        <attr name="si_no_dash" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="TicketView">
        <attr name="ticketOrientation" />
        <attr name="ticketBackgroundColor" />
        <attr name="ticketScallopRadius" />
        <attr name="ticketScallopPositionPercent" />
        <attr name="ticketShowBorder" />
        <attr name="ticketBorderWidth" />
        <attr name="ticketBorderColor" />
        <attr name="ticketShowDivider" />
        <attr name="ticketDividerWidth" />
        <attr name="ticketDividerPadding" />
        <attr name="ticketDividerColor" />
        <attr name="ticketDividerType" />
        <attr name="ticketDividerDashGap" />
        <attr name="ticketDividerDashLength" />
        <attr name="ticketCornerType" />
        <attr name="ticketCornerRadius" />
        <attr name="ticketElevation" />
        <attr name="android:elevation" />
        <attr name="ticketBackgroundBeforeDivider" format="reference" />
        <attr name="ticketBackgroundAfterDivider" format="reference" />
        <attr name="ticketShadowColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="ZigzagView">
        <attr name="zigzagHeight" format="dimension|reference" />
        <attr name="zigzagElevation" format="dimension|reference" />
        <attr name="zigzagBackgroundColor" format="color|reference" />
        <attr name="zigzagPaddingContent" format="dimension|reference" />
        <attr name="zigzagPadding" format="dimension|reference" />
        <attr name="zigzagPaddingLeft" format="dimension|reference" />
        <attr name="zigzagPaddingRight" format="dimension|reference" />
        <attr name="zigzagPaddingBottom" format="dimension|reference" />
        <attr name="zigzagPaddingTop" format="dimension|reference" />
        <attr name="zigzagSides">
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
            <flag name="right" value="4" />
            <flag name="left" value="8" />
        </attr>
        <attr name="zigzagShadowAlpha" format="float|reference" />
    </declare-styleable>

</resources>
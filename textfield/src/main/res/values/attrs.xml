<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InputMaskedEditText">
        <attr name="masked" format="string" />
    </declare-styleable>

    <declare-styleable name="MaskEditText">
        <attr name="mask" format="string" />
        <attr name="maskCharacter" format="string" />
        <attr name="maskStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="completable" value="1" />
            <enum name="persistent" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MaterialSpinner">
        <attr name="android:enabled" />
        <attr name="android:focusable" />
        <attr name="android:focusableInTouchMode" />
        <attr name="android:prompt" />
        <attr name="android:src" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="android:text" />
        <attr name="android:gravity" />
        <attr name="srcCompat" />
        <attr name="spinnerMode" format="enum">
            <enum name="dialog" value="0" />
            <enum name="dropdown" value="1" />
            <enum name="bottom_sheet" value="2" />
        </attr>
    </declare-styleable>

</resources>